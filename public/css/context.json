{
  "context": {
    "name": "@sys-person"
  },
  "output": {
    "generic": [
      {
        "title": "These are all the employees at Northrop Grumman that match your name:",
        "options": [
          {
            "label": "Example Option Name",
            "value": {
              "input": {
                "text": "Option Redirect Target"
              }
            }
          }
        ],
        "preference": "button",
        "description": "Please click the name of the person you want to find:",
        "response_type": "option"
      }
    ]
  },
  "actions": [
    {
      "name": "people_finder",
      "type": "client",
      "parameters": {
        "person_name": "$name"
      },
      "result_variable": "context.result"
    }
  ]
}




